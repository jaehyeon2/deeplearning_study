   "source": [
    "def softmax(a):\n",
    "    c = np.max(a)\n",
    "    exp_a = np.exp(a-c)\n",
    "    sum_exp_a = np.sum(exp_a)\n",
    "\n",
    "    y = exp_a/sum_exp_a\n",
    "    return y"
   ],
   "outputs": [],
   "execution_count": 4
  },
  {
   "metadata": {
